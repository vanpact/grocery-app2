{
  "name": "grocery-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint \"src/**/*.{ts,tsx}\" \"tests/**/*.ts\"",
    "format": "prettier --check \"**/*.{ts,tsx,md,json}\"",
    "db:setup": "tsx scripts/db-setup.ts",
    "db:setup:provision": "tsx scripts/db-setup.ts --provision-accounts",
    "start:android:emulator": "tsx scripts/start-android-emulator.ts",
    "start:android:device": "tsx scripts/start-android-device.ts",
    "verify:quick": "tsx scripts/verify-quick.ts",
    "verify:full": "tsx scripts/verify-full.ts",
    "verify:quick-wins": "tsx scripts/verify-quick-wins.ts",
    "verify:release-readiness": "tsx scripts/verify-release-readiness.ts",
    "test:runnable": "vitest run us1- us2- us3- tests/ux/us2- tests/contract/us3- tests/security/us3-",
    "test:runnable:regression": "npm run test:runnable"
  },
  "dependencies": {
    "@types/react": "~19.1.10",
    "expo": "^54.0.0",
    "expo-asset": "~12.0.12",
    "firebase": "^11.0.0",
    "firebase-admin": "^13.0.0",
    "react": "19.1.0",
    "react-dom": "^19.1.0",
    "react-native": "0.81.5",
    "react-native-paper": "^5.12.0",
    "react-native-web": "^0.21.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "eslint": "^8.57.1",
    "prettier": "^3.4.2",
    "tsx": "^4.20.4",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  }
}
